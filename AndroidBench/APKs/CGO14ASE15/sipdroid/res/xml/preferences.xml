    <PreferenceScreen android:title="@string/settings_notifications">
        <CheckBoxPreference android:title="@string/settings_MWI" android:key="MWI_enabled" android:summary="@string/settings_MWI2" />
        <CheckBoxPreference android:title="@string/settings_reg" android:key="registration" android:summary="@string/settings_reg2" />
        <CheckBoxPreference android:title="@string/settings_notify" android:key="notify" android:summary="@string/settings_notify2" />
        <CheckBoxPreference android:title="@string/settings_nodata" android:key="nodata" android:summary="@string/settings_nodata2" />
