    </PreferenceScreen>
    <PreferenceScreen android:title="@string/settings_av_options">
        <ListPreference android:entries="@array/eargain_display_values" android:title="@string/settings_eargain" android:key="eargain" android:defaultValue="0.25" android:dialogTitle="@string/settings_eargain" android:entryValues="@array/eargain_values" />
