<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceScreen android:title="@string/settings_account" android:key="account">
        <EditTextPreference android:singleLine="true" android:title="@string/settings_username" android:key="username" />
        <EditTextPreference android:password="true" android:singleLine="true" android:title="@string/settings_password" android:key="password" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_server" android:key="server" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_domain" android:key="domain" android:summary="@string/settings_domain2" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_username1" android:key="fromuser" android:summary="@string/settings_callerid2" />
        <ListPreference android:entries="@array/port_display_values2" android:title="@string/settings_port" android:key="port" android:dialogTitle="@string/settings_port" android:entryValues="@array/port_values2" />
        <ListPreference android:entries="@array/protocol_display_values2" android:title="@string/settings_protocol" android:key="protocol" android:dialogTitle="@string/settings_protocol" android:entryValues="@array/protocol_values2" />
        <CheckBoxPreference android:title="@string/settings_wlan" android:key="wlan" />
        <CheckBoxPreference android:title="@string/settings_3g" android:key="3g" android:summary="@string/settings_check" />
        <CheckBoxPreference android:title="@string/settings_edge" android:key="edge" android:summary="@string/settings_check" />
        <CheckBoxPreference android:title="@string/settings_vpn" android:key="vpn" android:summary="@string/settings_vpn2" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/settings_account" android:key="account1">
        <EditTextPreference android:singleLine="true" android:title="@string/settings_username" android:key="username1" />
        <EditTextPreference android:password="true" android:singleLine="true" android:title="@string/settings_password" android:key="password1" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_server" android:key="server1" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_domain" android:key="domain1" android:summary="@string/settings_domain2" />
        <EditTextPreference android:singleLine="true" android:title="@string/settings_username1" android:key="fromuser1" android:summary="@string/settings_callerid2" />
