        <CheckBoxPreference android:title="@string/settings_pos" android:key="pos" android:summary="@string/settings_pos2" />
        <CheckBoxPreference android:title="@string/settings_callback" android:key="callback" android:summary="@string/settings_callback2" />
        <CheckBoxPreference android:title="@string/settings_callthru" android:key="callthru" android:summary="@string/settings_callback2" />
