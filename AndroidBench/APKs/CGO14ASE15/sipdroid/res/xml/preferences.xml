        <ListPreference android:entries="@array/vquality_display_values" android:title="@string/settings_vquality" android:key="vquality" android:defaultValue="low" android:dialogTitle="@string/settings_vquality" android:entryValues="@array/vquality_values" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/codecs" android:key="codecs">
        <intent android:targetPackage="org.sipdroid.sipua" android:action="android.intent.action.MAIN" android:targetClass="org.sipdroid.codecs.Codecs$CodecSettings" />
    </PreferenceScreen>
