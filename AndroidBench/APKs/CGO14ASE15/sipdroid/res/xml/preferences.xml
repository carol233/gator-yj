        <CheckBoxPreference android:title="@string/settings_vpn" android:key="vpn1" android:summary="@string/settings_vpn2" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/settings_options">
        <ListPreference android:entries="@array/pref_display_values" android:title="@string/settings_pref" android:key="pref" android:summary="@string/settings_pref2" android:dialogTitle="@string/settings_pref" android:entryValues="@array/pref_values" />
        <CheckBoxPreference android:title="@string/settings_auto_on" android:key="auto_on" android:summary="@string/settings_auto_on2" />
        <CheckBoxPreference android:title="@string/settings_auto_ondemand" android:key="auto_ondemand" android:summary="@string/settings_auto_ondemand2" />
        <CheckBoxPreference android:title="@string/settings_auto_headset" android:key="auto_headset" android:summary="@string/settings_auto_headset2" />
        <CheckBoxPreference android:title="@string/settings_callrecord" android:key="callrecord" android:summary="@string/settings_callrecord2" />
