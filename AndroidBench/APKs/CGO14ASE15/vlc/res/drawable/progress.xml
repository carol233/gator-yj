<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#aaaaaaaa" android:endColor="#aaaaaaaa" android:angle="270.0" android:centerColor="#aadddddd" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffee8800" android:endColor="#ffaa4400" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>